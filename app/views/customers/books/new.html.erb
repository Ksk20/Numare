<div class="container">
  <div class="row">
  	<div class="col-lg">

  		<%= form_with model: @book do |f| %>

  			<%= render 'layouts/error_messages', model: f.object %>
			<h4>タイトル</h4>
  			<%= f.text_field :title %>
  			<h4>説明文</h4>
  			<%= f.text_area :caption %>
  			 <h4>書誌ID</h4>
  			<%= f.text_area :image_url %>
  			<%= f.submit '投稿' %>
		<% end %>

	</div>
  </div>
 </div>
